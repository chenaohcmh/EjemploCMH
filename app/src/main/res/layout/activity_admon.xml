<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".AdmonActivity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/imbackg03">



    <androidx.appcompat.widget.Toolbar
        android:id="@+id/My_toolbar_admon"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:titleTextColor="@color/black"
        android:background="@color/white"
        android:elevation="4dp"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        android:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>




    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragment_container_view_admon"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:name="com.example.ejemplocmh.Prom_DiaFragment"
        tools:layout="@layout/fragment_prom_dia" />

</LinearLayout>
